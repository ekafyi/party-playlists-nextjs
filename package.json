{
  "name": "with-typescript-eslint-jest",
  "author": "@erikdstock",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "yarn lint:tsc && yarn lint:eslint && yarn lint:prettier",
    "lint:tsc": "tsc --pretty --noEmit",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "lint:conflict": "eslint-config-prettier .eslintrc.json",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "fix:prettier": "prettier --write .",
    "test": "jest",
    "test-all": "yarn lint && yarn test"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn fix"
    ]
  },
  "dependencies": {
    "next": "latest",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intersection-observer": "^8.31.1",
    "react-web-share": "^1.1.0",
    "slugify": "^1.5.1",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/spotify-api": "^0.0.8",
    "@types/spotify-web-api-node": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.6.3",
    "eslint": "^7.0.0-rc.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.23.2",
    "husky": "^6.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.3",
    "lint-staged": "^10.5.4",
    "next-pwa": "^5.2.21",
    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4"
  }
}
